        "class Student:\n",
        "  def __init__(self, full_name, student_number, age, course):\n",
        "    self.full_name = full_name\n",
        "    self.student_number = student_number\n",
        "    self.age = age\n",
        "    self.course = course\n",
        "  def description(self):\n",
        "    return self.full_name, self.student_number, self.age, self.course\n",
        "  \n",
        "  def display(self):\n",
        "    print(\"Full Name:\", self.full_name)\n",
        "    print(\"Student Number:\" , self.student_number)\n",
        "    print(\"Age:\" , self.age)\n",
        "    print(\"Course:\" , self.course)\n",
        "name1 = Student(\"Exequiel Osias O.Comia\", \"20211150\", \"19\", \"B.S. Computer Engineering\")\n",
        "print(name1.display())"
